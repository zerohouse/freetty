var Article = mongoose.model('article', mongoose.Schema({
    done: Boolean,
    head: String,
    body: String,
    provider: String,
    date: Date,
    total: Object,
    discount: Object,
    selectedServices: Array,
    tags: Array,
    photos: Array,
    likes: Array,
    reply: Number,
    hits: Number,
    location: Object
}));